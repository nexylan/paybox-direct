{
    "name": "nexylan/paybox-direct",
    "description": "PayBox Direct and Direct Plus API wrapper",
    "keywords": ["Paybox", "Direct", "API", "Symfony"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sullivan SENECHAL",
            "email": "soullivaneuh@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/options-resolver": "^2.7 || ^3.0",
        "symfony/validator": "^2.7 || ^3.0",
        "doctrine/annotations": "^1.2",
        "doctrine/cache": "^1.0",
        "greg0ire/enum": "^2.1"
    },
    "require-dev": {
        "symfony/http-kernel": "^2.7 || ^3.0",
        "symfony/dependency-injection": "^2.7.10 || ^3.0",
        "phpunit/phpunit": "^5.3",
        "matthiasnoback/symfony-dependency-injection-test": "^0.7.6",
        "sllh/php-cs-fixer-styleci-bridge": "^2.0"
    },
    "suggest": {
        "symfony/http-kernel": "For Symfony integration as a bundle",
        "symfony/dependency-injection": "For Symfony integration as a bundle"
    },
    "conflict": {
        "symfony/http-kernel": "<2.7|>=4.0",
        "symfony/dependency-injection": "<2.7.10|>=4.0"
    },
    "autoload": {
        "psr-4": { "Nexy\\PayboxDirect\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Nexy\\PayboxDirect\\Tests\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
