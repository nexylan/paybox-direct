{
    "name": "nexylan/paybox-direct",
    "description": "PayBox Direct and Direct Plus API wrapper",
    "keywords": ["Paybox", "Direct", "API", "Symfony"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sullivan SENECHAL",
            "email": "soullivaneuh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "doctrine/annotations": "^1.2",
        "doctrine/cache": "^1.0",
        "greg0ire/enum": "^2.1 || ^3.0 || ^4.0",
        "guzzlehttp/guzzle": "^6.5",
        "paragonie/random_compat": "^2.0",
        "symfony/options-resolver": "^4.0",
        "symfony/validator": "^4.0 || ^5.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^4.2.1",
        "matthiasnoback/symfony-config-test": "^4.2.1",
        "phpunit/phpunit": "^9",
        "symfony/dependency-injection": "^4.0 || ^5.0",
        "symfony/http-kernel": "^4.0 || ^5.0"
    },
    "suggest": {
        "symfony/http-kernel": "For Symfony integration as a bundle",
        "symfony/dependency-injection": "For Symfony integration as a bundle"
    },
    "conflict": {
        "symfony/http-kernel": "<4.0|>=6.0",
        "symfony/dependency-injection": "<4.0|>=6.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Nexy\\PayboxDirect\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Nexy\\PayboxDirect\\Tests\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
